{
  "hosting": {
    "target": "minpham-com",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [{
      "source": "**",
      "dynamicLinks": true
    }]
  }
}
